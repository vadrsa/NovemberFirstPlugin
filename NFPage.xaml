<Pages:GridBasePage  
             xmlns:Pages="clr-namespace:Uniconta.ClientTools.Page;assembly=ClientTools"
             x:Class="NovemberFirstPlugin.NFPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
            xmlns:wpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:UnicontaControls="clr-namespace:Uniconta.ClientTools.Controls;assembly=ClientTools"
             xmlns:local="clr-namespace:NovemberFirstPlugin"
             mc:Ignorable="d" 
             xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"  
             d:DesignHeight="300" d:DesignWidth="300">

    <Grid x:Name="LayoutRoot" >
        <DockPanel  Width="Auto" VerticalAlignment="Stretch"     HorizontalAlignment="Stretch">
            <UnicontaControls:UnicontaRibbonControl x:Name="localMenu" DockPanel.Dock="Top"/>
            <wpf:ChromiumWebBrowser x:Name="myWebBrowser"
                                    DockPanel.Dock="Bottom"></wpf:ChromiumWebBrowser>
        </DockPanel>
    </Grid>
</Pages:GridBasePage>
